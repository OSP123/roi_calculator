		  <div id="web_ad_traffic_source" class="traffic_source">
	    <p>Web Ads</p>
			    Cost Per Click: <input id="cpc_web_ad" type="number" value="5"><br>
	  			Visitors: <input id="visitors_web_ad" type="number" value="100000" ><br>
	  			Total Visitors: <input id="total_visitors_web_ad" type="number" value="100000" ><br>
		  </div>
		  <div id="email_traffic_source" class="traffic_source">
	    <p>Emails</p>
			    Click Through Rate: <input id="ctr_email" type="number" value="5"><br>
	  			Visitors: <input id="emails" type="number" value="100000"><br>
	  			Total Visitors: <input id="total_visitors_emails" type="number" value="5000"><br>
		  </div>
		  <div id="website_traffic_source" class="traffic_source">
	    <p>Website</p>
			    Click Through Rate: <input id="ctr_website" type="number" value="5"><br>
	  			Total: <input id="visitors_website" type="number" value="100000"><br>
	  			Total Visitors: <input id="total_visitors_website" type="number" value="5000"><br>
		  </div>
		  <div id="facebook_ad_traffic_source" class="traffic_source">
	    <p>Facebook Ads</p>
			    Cost Per Click: <input id="cpc_fb_ad" type="number" value="3"><br>
	  			Total: <input id="visitors_fb_ad" type="number" value="100000"><br>
	  			Total Visitors: <input id="total_visitors_fb_ad" type="number" value="100000"><br>
		  </div>

	    <div id="total_cost_container">
		    <div><p>Total Cost:</p><input id="total_cost" type="number" value="20000"></div>
		  </div>
		</div>
		<div id="conversion_container" class="sub_container">
